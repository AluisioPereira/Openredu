<ol id="breadcrumbs" class="clearfix">
  <li>
    <%= link_to 'Home', home_user_path(current_user),
      :class => 'replacement home icon icon-home-lightblue_16_16' %>
  </li>
  <li>
    <%= link_to @subject.space.course.environment.name,
      environment_path(@subject.space.course.environment),
      :class => 'replacement icon icon-environment-lightblue_16_16' %>
  </li>
  <li>
    <%= link_to @subject.space.course.name,
      environment_course_path(@subject.space.course.environment, @subject.space.course),
      :class => 'replacement icon icon-course-lightblue_16_16' %>
  </li>
  <li>
    <%= link_to @subject.space.name, space_path(@subject.space),
      :class => 'replacement icon icon-space-lightblue_16_16' %>
  </li>
  <li>
    <%= link_to @subject.title, space_subject_path(@subject.space, @subject),
      :class => 'icon icon-subject-lightblue_16_16'%>
  </li>
</ol>
