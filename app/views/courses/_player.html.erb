<% if @seminar.video? %>
	<%= render :partial => 'courses/video', :locals => {:resource => @seminar, :is_interactive_class => is_interactive_class} %>
<% elsif @seminar.audio? %>
	<%= render :partial => 'courses/audio', :locals => {:resource => @seminar} %>
<% elsif @seminar.external_resource_type.eql?('youtube')%>
	<%= render :partial => "courses/youtube", :locals => {:resource => @seminar, :is_interactive_class => is_interactive_class} %>
<% else %>
	<%= link_to "Download", @seminar.media.url %>
<% end %>

