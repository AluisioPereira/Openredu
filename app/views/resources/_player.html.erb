<% if resource.video? %>
	<%= render :partial => 'resources/video', :locals => {:resource => resource} %>
<% elsif resource.audio? %>
	<%= render :partial => 'resources/audio', :locals => {:resource => resource} %>
<% elsif resource.type.eql?('youtube') %>
	<%= render :partial => "resources/youtube", :locals => {:resource => resource} %>
<% else %>
	<%= link_to "Download", resource.media.url %>
<% end %>

