items = '<%= escape_javascript render(:partial => partial, :collection => collection, :locals => partial_locals) %>'
$('<%= raw selector %>').append(items);

endless = '<%= will_paginate collection, paginate_opts.merge(:renderer => EndlessRenderer) %>'
<% if paginate_opts.has_key? :id %>
  $('#<%= paginate_opts[:id] %>').replaceWith(endless);
<% else %>
  $('.endless').replaceWith(endless);
<% end %>
