$('.errorMessageField').remove();
$('.spinner').hide();

<% if @folder.valid? %>
  <%= render :partial => "folders/index" %>
<% else %>
  var error = $("<div class='errorMessageField'>").html(" <%= escape_javascript @folder.errors.full_messages.join(",") %>");
  $('.new-folder-inner form').prepend(error);
<% end %>
