<% unless @partner_contact.valid? %>
  <% partial = (render :partial => 'partners/partner_contacts/new', :locals => { :environment => @environment }) %>
  var $form = $("#partner-contact-form");
  $form.replaceWith("<%= escape_javascript(partial) %>");
<% else %>
  window.location = "<%= success_partner_path(@partner) %>";
<% end %>
