<% if status.status? or
(status.log && (status.logeable.published.nil? ||
                (status.logeable.published && status.logeable.published?))) %>
    <%= render :partial => "statuses/status", :locals => {:status_counter => type_proxy_counter, :status => status, :statusable => statusable } %>
<% elsif status.question? %>
    <%= render :partial => "statuses/question", :locals => {:question => status, :statusable => statusable } %>
<% elsif status.poll? %>

<% end%>

