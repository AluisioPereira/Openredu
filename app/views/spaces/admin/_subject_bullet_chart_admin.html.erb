<% content_tag(:div, :class => "subject-participation-bullet",
               :id => "subject-participation-bullet-#{subject.id}") do %>
  <%= lazy_load(:chart, :jammit => true) do %>
    try{
      var bulletSbjPar = subjectParticipationBullet();
      bulletSbjPar.load(<%= subject.id %>,
                        "<%= api_vis_subject_activities_path(subject.id) %>",
                        "#subject-participation-bullet-<%= subject.id %>");
    }catch(e){
    }
  <% end %>
<% end %>
