<%= render :partial => 'spaces/new/sidebar', :locals => { :space => @space } %>
<div class="main grid_12 omega big-col">
  <%= render :partial => "spaces/new/breadcrumbs", :locals => { :space => @space } %>
  <%= render :partial => 'spaces/new/header',
    :locals => { :space => @space }%>

    <div id="space-manage" class="body">
      <div class="header">
        <h4>Gerenciamento</h4>
        <span class="counter">Lista de Notícias pendentes e publicados na Disciplina <%= @space.name %></span>
      </div>

      <% fake_tabs ["Informações", edit_space_path(@space), { :class => "icon icon-management-gray_16_16" }],
        ["Eventos", admin_events_space_path(@space), { :class => "icon icon-event-gray_16_16" }],
        ["Membros", admin_members_space_path(@space), { :class => "icon icon-members-gray_16_16" }],
        ["Notícias", admin_bulletins_space_path(@space), { :class => "icon icon-bulletin-lightblue_16_16" }] do %>

        <%= render :partial => 'bulletins/moderate_index_admin' %>
        <%= render :partial => 'bulletins/index_admin' %>
      <% end %>
    </div><!-- end #space-manage -->

  </div><!-- end .main -->


