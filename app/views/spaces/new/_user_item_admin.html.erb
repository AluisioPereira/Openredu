<tr id="user-<%= user_item_admin.user_id %>">
  <td>
    <%= image_tag user_item_admin.user.avatar(:thumb_32),
          :class => "avatar" %>
    <%= link_to user_item_admin.user.display_name, user_item_admin.user,
      :rarget => "_blank", :class => "user" %>
  </td>
  <td>
    <% if user_item_admin.role == Role[:member] %>
      <span class="role icon icon-redu_r8_c355_s1">Aluno</span>
    <% elsif user_item_admin.role == Role[:teacher] %>
      <span class="role icon icon-redu_r8_c207_s1">Professor</span>
    <% elsif user_item_admin.role == Role[:tutor] %>
      <span class="role icon icon-redu_r8_c385_s1">Tutor</span>
    <% elsif user_item_admin.role == Role[:environment_admin] %>
      <span class="role icon ">Administrador</span>
    <% end %>
  </td>
  <td>
    <span class="date"><%= time_ago_in_words(user_item_admin.created_at) %></span>
  </td>
</tr>

