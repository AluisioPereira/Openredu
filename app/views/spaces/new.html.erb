<%= render :partial => 'courses/sidebar' %>
<div class="main grid_12 omega big-col">
  <%= render :partial => "courses/breadcrumbs",
    :locals => { :course => @course } %>
  <%= render :partial => 'courses/header',
      :locals => { :course => @course }%>

    <div id="space-new" class="hierarchy-listing-wrapper">
      <div class="header">
        <h4>Disciplina</h4>
        <span class="counter">Nova disciplina</span>
      </div>

      <% form_for @space, :html => {:class => "form-common highlightable",
        :id => "form_school"} do |f|%>
        <%= render "spaces/form", :f => f, :edit => false %>
        <%= f.submit "Criar disciplina" %>
      <% end %>
    </div><!-- end #space-new -->
  </div><!-- end main -->
