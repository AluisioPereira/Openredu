<%#
  Exibe o mural com os status.

  Variáveis:
    user: o usuário.
    status: o status.
    statuses: os status.
%>

<% if user.settings.visited?(home_user_path(user)) %>
  <%= render "statuses/wall", status: status, statusable: user,
    placeholder: "Compartilhe algo com seus Amigos através de seu mural",
    statuses: statuses %>
<% end %>