<h1>Listing access_keys</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Expiration date</th>
  </tr>

<% @access_keys.each do |access_key| %>
  <tr>
    <td><%=h access_key.key %></td>
    <td><%=h access_key.expiration_date %></td>
    <td><%= link_to 'Show', access_key %></td>
    <td><%= link_to 'Edit', edit_access_key_path(access_key) %></td>
    <td><%= link_to 'Destroy', access_key, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New access_key', new_access_key_path %>