<ul class="upcoming-events">
  <% upcoming.each do |event| %>
  <li class="clearfix">
    <div class="date">
      <span class="quiet day"><%= event.start_time.day %></span>
      <span class="col">
        <span class="month quiet"><%= event.start_time.strftime("%b") %></span>
        <span class="quiet year"><%= event.start_time.year %></span>
      </span>    
    </div>
    <span class="name"><%= link_to event.name, space_event_path(event.space, event) %></span>
  </li>
  <% end %>
</ul>
