<%= error_messages_for :client_application %>
<div class="control-group">
  <div class="control-label">
    <label for="client_application_name">Nome *</label>
  </div>
  <div class="control">
    <%= f.text_field :name %>
  </div>
</div>
<div class="control-group">
  <div class="control-label">
    <label for="client_application_url">URL Principal*</label>
  </div>
  <div clas="control">
    <%= f.text_field :url %>
  </div>
</div>
<div class="control-group">
  <div class="control-label">
    <label for="client_application_callback_url">URL de Retorno (deixe em branco para desktop)</label>
  </div>
  <div class="control">
    <%= f.text_field :callback_url %>
  </div>
</div>
<div class="control-group">
  <div class="control-label">
    <label for="client_application_support_url">URL de Suporte</label>
  </div>
  <div class="control">
    <%= f.text_field :support_url %>
  </div>
</div>
