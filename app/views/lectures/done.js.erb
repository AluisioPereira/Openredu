<% if @done == 0 %>
  $('#done').val('1');
  $('#do_lecture').removeClass("done");
  $('#do_lecture').addClass("undone");
  $('#do_lecture > fieldset > label').removeClass("icon-class-green_16_18-before");
  $('#do_lecture > fieldset > label').addClass("icon-class-gray_16_18-before");
<% else %>
  $('#done').val('0');
  $('#do_lecture').removeClass("undone");
  $('#do_lecture').addClass("done");
  $('#do_lecture > fieldset > label').removeClass("icon-class-gray_16_18-before");
  $('#do_lecture > fieldset > label').addClass("icon-class-green_16_18-before");
<% end %>

$('#progress_bar > div > .completed').attr('style', "width: <%= @student_grade %>%");
$('#progress_bar > .progress-value').html("<%= @student_grade  %>%");
