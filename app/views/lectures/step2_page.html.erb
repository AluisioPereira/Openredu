<div class="grid_9 push_3">
  <table class="booking_steps" cellpadding="0" cellspacing="0">
    <tbody><tr>
        <td><span class="book_step disabled"><span><em>1</em>Informações Gerais</span></span></td>
        <td class="spacer">&nbsp;</td>
        <td><span class="book_step current"><span><em>2</em>Editor de Aula</span></span></td>
        <td class="spacer">&nbsp;</td>
        <td><span class="book_step disabled"><span><em>3</em>Publicação</span></span></td>
      </tr>
  </tbody></table>
</div>
<div class="grid_15">
  <h3 class="call">Novo artigo </h3>
</div>



<%= form_for :page, @page, :url => { :action => "create" }, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>

  <%= hidden_field_tag("step", "2", options = {})%>

  <div class="grid_16">
    <%= f.cktext_area :body, :width => "930px", :height => "400px" %>
  </div>

  <div class="grid_16">
    <%= f.submit '< Anterior', :name => "back_button" %>
    <%= f.submit 'Próximo >' %> ou <%= link_to 'Cancelar', cancel_space_subject_lectures_path(@space, @subject), :confirm => "Tem certeza que deseja sair do editor de aulas?" %>
  </div>
<% end %>

</div>



</div>
