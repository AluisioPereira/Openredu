<% if seminar.external_resource_type.eql?('upload')%>
  <%= render :partial => 'lectures/new/video', :locals => { :resource => seminar } %>
<% elsif seminar.external_resource_type.eql?('youtube')%>
  <%= render :partial => "lectures/new/youtube", :locals => { :resource => seminar } %>
<% else %>
  <%= link_to "Download", lecture.media.url %>
<% end %>

